<%- include("./includes/head.ejs") %>

  <body>
    <h1>
      name is: @<%= data.name %>
    </h1>
    <h3>followers: <%= data.followers %>
    </h3>
    <h3>following: <%= data.following %>
    </h3>
    <hr>

    <% for( let post of data.posts ) { %>
      <img src="<%= post.image %>" alt="">
      <p>Likes: <%= post.likes %>
      </p>
      <p>Comment: <%= post.comments %>
      </p>
      <% } %>

        <footer>
          <%- include("./includes/footer.ejs") %>
        </footer>
  </body>

  </html>